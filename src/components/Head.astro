---
export interface Props {
	title: string;
}

const { title } = Astro.props;
---

<head>
	<meta charset="UTF-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>
		{title}
	</title>
	<meta
		name="description"
		content="Digital Help es un emprendimiento que se ocupa de asistir a las instituciones educativas en toda el área de
	tecnología. Reparación, actualización, mantenimiento de equipos informáticos y demás dispositivos tecnológicos"
	/>
	<meta property="og:title" content="Digital help" />
	<meta
		property="og:description"
		content="Asistencia a instituciones educativas. Soluciones tecnológicas, mantenimiento y reparación"
	/>
	<meta
		property="og:image"
		content="https://www.digitalhelp.com.ar/og-image.png"
	/>
	<link rel="icon" href="/favicon.png" type="image/png" />
</head>
